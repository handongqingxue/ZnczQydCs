<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.znczQydCs.dao.DaYinJiLuMapper">
	<insert id="add" parameterType="com.znczQydCs.entity.DaYinJiLu">
		insert into da_yin_ji_lu
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="html!=null">
				html,
			</if>
			<if test="time!=null">
				time,
			</if>
		</trim>
		values
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="html!=null">
				#{html},
			</if>
			<if test="time!=null">
				#{time},
			</if>
		</trim>
	</insert>
	<select id="selectByTime" resultType="com.znczQydCs.entity.DaYinJiLu">
		select * from da_yin_ji_lu where time=#{time}
	</select>
</mapper>